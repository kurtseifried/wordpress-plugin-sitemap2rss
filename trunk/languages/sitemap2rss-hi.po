#. Plugin Name of the plugin
#: sitemap2rss.php
msgid "sitemap2rss"
msgstr "साइटमैप से आरएसएस"

#. Plugin URI of the plugin
#: sitemap2rss.php
msgid "https://github.com/kurtseifried/wordpress-plugin-sitemap2rss"
msgstr "https://github.com/kurtseifried/wordpress-plugin-sitemap2rss"

#. Description of the plugin
#: sitemap2rss.php
msgid "Convert XML sitemaps into RSS feeds using predefined aliases."
msgstr "पूर्वनिर्धारित उपनामों का उपयोग करके XML साइटमैप को RSS फीड में परिवर्तित करें।"

#. Author of the plugin
#: sitemap2rss.php
msgid "Kurt Seifried"
msgstr "कर्ट सिफ्राइड"

